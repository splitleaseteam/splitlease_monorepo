{
  "name": "split-lease",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "check": "node scripts/build-checks.js",
    "dev": "bun run check && vite --port 3000",
    "dev:test": "(bun run lint || true) && vite --port 8010 --strictPort",
    "dev:test:restart": "bun run dev:test:stop && bun run dev:test",
    "dev:test:stop": "node scripts/kill-port.cjs 8010",
    "generate-routes": "node scripts/generate-redirects.js",
    "knip": "knip",
    "knip:report": "knip || true",
    "lint": "eslint src/",
    "lint:check": "eslint src/ --max-warnings 0",
    "lint:fix": "eslint src/ --fix",
    "prebuild": "bun run generate-routes",
    "preview": "vite preview --port 3000",
    "build": "bun run check && vite build",
    "build:dev": "(bun run lint || true) && vite build --mode development",
    "deploy": "node scripts/check-branch.js --require-main && wrangler pages deploy dist --project-name splitlease",
    "deploy:dev": "node scripts/check-branch.js --block-main && bun run build:dev && wrangler pages deploy dist --project-name splitlease --branch development",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:dev": "bun run test:stop && (bun run lint || true) && bun run knip:report && vite --port 8001 --strictPort",
    "test:stop": "node scripts/kill-port.cjs 8001",
    "test:unit": "vitest",
    "test:unit:run": "vitest run",
    "test:unit:coverage": "vitest run --coverage",
    "test:regression": "vitest run src/__tests__/regression",
    "typecheck": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build -o storybook-static"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@react-google-maps/api": "^2.20.7",
    "@stripe/react-stripe-js": "^5.6.0",
    "@stripe/stripe-js": "^8.7.0",
    "@supabase/supabase-js": "^2.38.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "framer-motion": "^12.23.24",
    "gsap": "^3.14.2",
    "knip": "^5.82.1",
    "lottie-react": "^2.4.1",
    "lucide-react": "^0.553.0",
    "node-releases": "^2.0.27",
    "pg": "^8.17.2",
    "postgres": "^3.4.8",
    "qrcode.react": "^4.2.0",
    "react": "^18.2.0",
    "react-datepicker": "^8.9.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.66.1",
    "react-to-print": "^3.2.0",
    "styled-components": "^6.1.19",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^8.4.7",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-onboarding": "^8.4.7",
    "@storybook/addon-viewport": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/react": "^8.4.7",
    "@storybook/react-vite": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^4.0.18",
    "autoprefixer": "^10.4.23",
    "dotenv": "^17.2.3",
    "esbuild": "^0.27.0",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^17.0.0",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "playwright": "1.51.0",
    "postcss": "^8.5.6",
    "storybook": "^8.4.7",
    "supabase": "^2.58.5",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.9.3",
    "vite": "^5.0.0",
    "vitest": "^4.0.18"
  }
}
