# E2E Test Environment Variables
# Copy this file to .env.test and fill in your values
# DO NOT commit .env.test to version control

# Supabase Configuration (DEV environment)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# E2E Test Configuration
E2E_BASE_URL=http://localhost:3000

# Optional: Delete test users after tests complete
# Set to 'true' to clean up users (slower but cleaner)
# Leave unset or 'false' to keep users (faster re-runs)
DELETE_TEST_USERS=false
