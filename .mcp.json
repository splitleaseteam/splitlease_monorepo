{
  "mcpServers": {
    "knip": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@knip/mcp"
      ]
    },
    "playwright": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@playwright/mcp"
      ]
    },
    "supabase": {
      "type": "http",
      "url": "https://mcp.supabase.com/mcp",
      "env": {
        "SUPABASE_URL": "https://qzsmhgyojmwvtjmnrdea.supabase.co",
        "SUPABASE_ANON_KEY": "${SUPABASE_ANON_KEY}",
        "SUPABASE_SERVICE_ROLE_KEY": "${SUPABASE_SERVICE_ROLE_KEY}"
      }
    }
  }
}