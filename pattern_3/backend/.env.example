# =====================================================
# ENVIRONMENT VARIABLES - EXAMPLE
# =====================================================
# Copy this file to .env and fill in your values
# =====================================================

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Database Configuration
DATABASE_URL=postgresql://postgres:password@localhost:54322/postgres

# Edge Functions
FUNCTIONS_URL=https://your-project.supabase.co/functions/v1

# Environment
NODE_ENV=development

# Testing
TEST_USER_ID=00000000-0000-0000-0000-000000000001
TEST_BOOKING_ID=00000000-0000-0000-0000-000000000002

# Analytics
ENABLE_ANALYTICS=true
ANALYTICS_SAMPLING_RATE=1.0

# Feature Flags
ENABLE_AB_TESTING=true
DEFAULT_TIER_MULTIPLIERS={"budget":0.90,"recommended":1.00,"premium":1.15}

# Caching
TIER_CACHE_TTL=3600
CALC_CACHE_TTL=300

# Rate Limiting
MAX_REQUESTS_PER_MINUTE=100
MAX_REQUESTS_PER_HOUR=1000

# Monitoring
SENTRY_DSN=
LOG_LEVEL=info
